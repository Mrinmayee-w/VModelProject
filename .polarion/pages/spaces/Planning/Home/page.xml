  <h3 id="polarion_client13">Published Releases</h3>
  <div id="polarion_client14" class="polarion-rp-widget-part" data-widget="com.polarion.scriptBlock">
    <span class="polarion-rp-widget-parameters">
      <sub id="script">
        <sub id="script" style="white-space:pre;">################################################################################
##
## Published Releases
##
## com.polarion.alm.shared.api.transaction.ReadOnlyTransaction $transaction
## com.polarion.alm.shared.api.model.wi.WorkItem $item
## com.polarion.alm.shared.api.model.rp.widget.RichPageRenderingContext $renderingContext
##

#set($projectId = $page.getReference().projectId())
#set($query = &quot;type:release AND status:published AND project.id:$projectId&quot;)
#set($sortBy = &quot;~publicLaunch&quot;)
#set($uniqueId = $renderingContext.generateUniqueElementId())
#set($link = $transaction.context().createPortalLink().project(&quot;$projectId&quot;).workItems().query(&quot;$query&quot;).sorting(&quot;$sortBy&quot;).toEncodedRelativeUrl())
#set($workItems = $transaction.workItems().search().query(&quot;$query&quot;).sort(&quot;$sortBy&quot;).limit(10))
#set($workItemCount = $workItems.size())
#set($main = $transaction.context().createPortalLink().project(&quot;$projectId&quot;).document(&quot;Planning&quot;, &quot;Release Report&quot;).toEncodedRelativeUrl())

## Sidebar Preparation START
#set($builderWithSidebar = $transaction.context().createHtmlFragmentBuilderFor().rpeView())
## Sidebar: Configure displayed fields - #set($displayedFields = [&quot;field1&quot;, &quot;field2&quot;, ...]
#set($displayedFields = [&quot;releasePlan&quot;, &quot;status&quot;, &quot;resolution&quot;, &quot;releasePM&quot;, &quot;releaseQA&quot;, &quot;featureFreezeDate&quot;, &quot;codeFreezeDate&quot;, &quot;publicLaunch&quot;, &quot;build&quot;, &quot;buildId&quot;, &quot;releaseTestEvidence&quot;, &quot;linkedWorkItems&quot;])
## Sidebar: Choose unique ID - #set($myContainerId = &quot;myUniqueId&quot;)
#set($myContainerId = &quot;publishedReleasesWithSidebar&quot;)
#set($sidebarConfiguration = $renderingContext.propertiesSidebarConfiguration().containerId(&quot;$myContainerId&quot;).fieldIds($displayedFields).addTo($builderWithSidebar))
#set($pathAttr = $sidebarConfiguration.attributes().referencePath())
#set($clickClass = $sidebarConfiguration.attributes().clickableCssClass())
## Sidebar Configuration END

## Sidebar: Assign a unique ID to the table that contains lines that should be clickable - &lt;table id=&quot;$myContainerId&quot;&gt;
&lt;table class=&quot;polarion-rpw-table-main&quot; id=&quot;$myContainerId&quot;&gt; 
	&lt;tbody&gt;
		&lt;tr&gt;
			&lt;td&gt;
				&lt;table class=&quot;polarion-rpw-table-content&quot;&gt;
					&lt;tbody&gt;
						&lt;tr class=&quot;polarion-rpw-table-header-wiki&quot;&gt;
							&lt;th&gt;Release&lt;/th&gt;
							&lt;th&gt;Public Launch&lt;/th&gt;
							&lt;th&gt;
								&lt;span&gt;
									Build&lt;br/&gt;
									&lt;small&gt;Build ID&lt;/small&gt;
								&lt;/span&gt;
							&lt;/th&gt;
							&lt;th&gt;
								&lt;span&gt;
									Release Plan&lt;br/&gt;
									&lt;small&gt;Status&lt;/small&gt;
								&lt;/span&gt;	
							&lt;/th&gt;
						&lt;/tr&gt;
						#foreach($workItem in $workItems)
							## Sidebar: Add click class to each row - &lt;tr class=&quot;$clickClass&quot;&gt;
							## Sidebar: Add path to the Work Item for each row - &lt;tr $pathAttr=&quot;$workItem.getReference().toPath()&quot;&gt;
							&lt;tr class=&quot;polarion-rpw-table-content-row $clickClass&quot; $pathAttr=&quot;$workItem.getReference().toPath()&quot;&gt;
								#set($suffix = &quot;?item=$workItem.getReference().id()&quot;)
								#set($itemLink = &quot;$main$suffix&quot;)
								&lt;td&gt;&lt;a target=&quot;_top&quot; href=&quot;$itemLink&quot;&gt;$workItem.render().withTitle()&lt;/a&gt;&lt;/td&gt;
								&lt;td&gt;$workItem.fields().get(&quot;publicLaunch&quot;).render()&lt;/td&gt;
								&lt;td&gt;
									&lt;span&gt;
										#if(!$workItem.fields().get(&quot;build&quot;).is().empty())
											#set($buildId = &quot;$workItem.fields().get(&#39;build&#39;).get().fields().id().get()&quot;)
											#set($buildLink = $transaction.context().createPortalLink().project(&quot;$projectId&quot;).build(&quot;$buildId&quot;).toEncodedRelativeUrl())
											&lt;a href=&quot;$buildLink&quot; target=&quot;_top&quot;&gt;$workItem.fields().get(&quot;build&quot;).render()&lt;/a&gt;
										#end
										&lt;br/&gt;
										&lt;small&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; $workItem.fields().get(&#39;buildId&#39;).render()&lt;/small&gt;
									&lt;/span&gt;
								&lt;/td&gt;
								#set($releasePlan = $transaction.plans().getBy().ids(&quot;$projectId&quot;, &quot;$workItem.fields().get(&#39;releasePlan&#39;).get().id()&quot;))
								&lt;td&gt;
									&lt;span&gt;
										$releasePlan.render().withLinks()&lt;br/&gt;
										&lt;small&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; $releasePlan.fields().status().render()&lt;/small&gt;
									&lt;/span&gt;
								&lt;/td&gt;
							&lt;/tr&gt;
						#end
					&lt;/tbody&gt;
				&lt;/table&gt;
			&lt;/td&gt;
		&lt;/tr&gt;
		&lt;tr&gt;
			&lt;td class=&quot;polarion-rpw-table-footer&quot;&gt;
				&lt;div class=&quot;polarion-rpw-table-counts&quot;&gt;
					&lt;a href=&quot;$link&quot; target=&quot;_top&quot;&gt;$workItemCount items found&lt;/a&gt;
				&lt;/div&gt;
				&lt;div class=&quot;polarion-rpw-table-open-in-table&quot;&gt;
					&lt;a href=&quot;$link&quot; target=&quot;_blank&quot;&gt;
						&lt;img src=&quot;/polarion/ria/images/portlet/portletOpenInTable.png&quot; title=&quot;Open in table&quot;&gt;
					&lt;/a&gt;
				&lt;/div&gt;
				&lt;div class=&quot;polarion-rpw-table-show-query&quot;&gt;
					&lt;img src=&quot;/polarion/ria/images/portlet/info.png&quot; title=&quot;Show query&quot; onclick=&quot;var style = getElementById(&#39;$uniqueId&#39;).style; style.display = (style.display == &#39;block&#39;) ? &#39;none&#39; : &#39;block&#39;;&quot;&gt;
				&lt;/div&gt;
				&lt;div id=&quot;$uniqueId&quot; class=&quot;polarion-rpw-table-query&quot;&gt;$query&lt;/div&gt;
			&lt;/td&gt;
		&lt;/tr&gt;
	&lt;/tbody&gt;
&lt;/table&gt;
## Sidebar: Render helper HTML fragment
$builderWithSidebar
        </sub>
      </sub>
    </span>
